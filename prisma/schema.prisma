generator client {
  provider              = "prisma-client-js"
}

datasource db {
  provider              = "mysql"
  url                   = env("DATABASE_URL")
  referentialIntegrity  = "prisma"
}

model Projects {
  id            Int      @id @default(autoincrement())
  title         String
  slug          String   @unique
  description   String
  image         String
  link_demo     String?
  link_github   String?
  stacks        String
  is_show       Boolean
  updated_at    DateTime @default(now())
}
